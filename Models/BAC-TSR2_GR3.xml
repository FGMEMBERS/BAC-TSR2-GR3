<!-- De-select pilot when in view 0 -->
 <animation>
  <type>select</type>
  <object-name>CPT-Pilot-Head</object-name>
  <object-name>CPT-Pilot-Body</object-name>
  <object-name>CPT-Helmet</object-name>
  <object-name>CPT-Visor</object-name>
  <object-name>CPT-Facemask</object-name>
  <condition>
   <not>
   <equals>
    <property>sim/current-view/view-number</property>
    <value>0</value>
   </equals>
   </not>
  </condition>
 </animation>

<!-- De-select Back-seater when in view 6 -->
 <animation>
  <type>select</type>
  <object-name>CPT-Pilot-Head2</object-name>
  <object-name>CPT-Pilot-Body2</object-name>
  <object-name>CPT-Helmet2</object-name>
  <object-name>CPT-Visor2</object-name>
  <object-name>CPT-Facemask2</object-name>
  <condition>
   <not>
   <equals>
    <property>sim/current-view/view-number</property>
    <value>6</value>
   </equals>
   </not>
  </condition>
 </animation>



